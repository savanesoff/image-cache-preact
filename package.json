{
  "name": "image-cache-preact",
  "private": false,
  "version": "0.4.3",
  "type": "module",
  "description": "Preact component for image caching and rendering performance boost. Provides ways to manage image loading, caching and memory usage (RAM & GPU).",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "license": "MIT",
  "author": {
    "name": "Samvel Avanesov",
    "email": "sam.avanesoff@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/savanesoff/image-cache-react"
  },
  "packageManager": "pnpm@9.1.4",
  "keywords": [
    "cache",
    "image",
    "asset-management",
    "image-cache",
    "image-memory",
    "memory",
    "ram",
    "video-memory",
    "memory-management",
    "preact",
    "preact-component",
    "preact-image-memory",
    "preact-resizable",
    "typescript",
    "typescript-component",
    "typescript-preact"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview --root dist-demo",
    "test": "vitest --run",
    "prepack": "pnpm run build",
    "clear:demo": "rm -rf dist-demo; mkdir dist-demo",
    "clean:package": "rm -rf ./dist",
    "build:types": "tspc --project tsconfig.package.json",
    "build:package": "pnpm run clean:package && vite build && pnpm run build:types",
    "build:demo": "pnpm clear:demo; bash ./ts-cmd.sh --path tsconfig.build.demo.json --cmd vite build --config vite.config.demo.ts",
    "deploy:demo": "pnpm run build:demo && gh-pages -d dist-demo",
    "build": "pnpm run build:package",
    "release": "pnpm run test && pnpm run build:package && pnpm changeset publish"
  },
  "dependencies": {
    "image-cache-pro": "^0.5.1",
    "preact": "^10.22.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-development": "^7.24.6",
    "@eslint/js": "^9.3.0",
    "@noriginmedia/norigin-spatial-navigation": "^2.1.0",
    "@preact/preset-vite": "^2.8.2",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/preact": "^3.2.4",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "babel-plugin-transform-hook-names": "^1.0.2",
    "changeset": "^0.2.6",
    "clsx": "^2.1.0",
    "declaration-bundler-webpack-plugin": "^1.0.3",
    "devtools-fps": "^2.0.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-webpack": "^0.13.8",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.5",
    "eslint-plugin-require-extensions": "^0.1.3",
    "gh-pages": "^6.1.1",
    "jsdom": "^24.0.0",
    "lorem-ipsum": "^2.0.8",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "rollup": "^4.17.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-transform-paths": "^3.4.7",
    "vite": "^5.2.11",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  }
}
