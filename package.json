{
  "name": "react-image-cache",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "description": "React component for image caching. Provides a way to handle load and cache of images in memory and manage their lifecycle in both GPU and RAM.",
  "author": {
    "name": "Samvel Avanesov",
    "email": "sam.avanesoff@gmail.com"
  },
  "license": "MIT",
  "packageManager": "^pnpm@8.6.6",
  "keywords": [
    "cache",
    "image",
    "asset-management",
    "image-cache",
    "image-memory",
    "memory",
    "ram",
    "video-memory",
    "memory-management",
    "react",
    "react-component",
    "react-image-memory",
    "react-resizable",
    "typescript",
    "typescript-component",
    "typescript-react"
  ],
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview --root dist-demo",
    "test": "vitest --run",
    "prepack": "pnpm run build",
    "clean": "rm -rf ./dist",
    "clear:demo": "rm -rf dist-demo; mkdir dist-demo",
    "build": "pnpm run clean && pnpm run build:esm",
    "build:esm": "tsc -p ./tsconfig.build.json && mv ./dist/index.js ./dist/index.mjs",
    "build:demo": "pnpm clear:demo; bash ./demo-build.sh --path tsconfig.build.demo.json",
    "deploy:demo": "pnpm run build:demo && gh-pages -d dist-demo"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "events": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.4.3",
    "react-intersection-observer": "^9.10.2",
    "lorem-ipsum": "^2.0.8"
  },
  "devDependencies": {
    "@noriginmedia/norigin-spatial-navigation": "^2.1.0",
    "@types/events": "^3.0.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "devtools-fps": "^2.0.2",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "jsdom": "^24.0.0",
    "lorem-ipsum": "^2.0.8",
    "path": "^0.12.7",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "react-intersection-observer": "^9.10.2",
    "typescript": "^5.0.2",
    "vite": "^5.2.8",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*"
  ]
}
